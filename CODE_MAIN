   "cell_type": "markdown",
   "id": "e55b7074",
   "metadata": {},
   "source": [
    "Data Pre-Processing"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 58,
   "id": "bba73811",
   "metadata": {},
   "outputs": [],
   "source": [
    "#performing one-hot encoding on my dataset\n",
    "def ohe_data(df):\n",
    "    columns_ohe = ['school', 'sex', 'address', 'famsize', 'Pstatus', 'Mjob', 'Fjob', 'reason', 'guardian', 'schoolsup', \n",
    "                   'famsup', 'paid', 'activities', 'nursery', 'higher', 'internet', 'romantic']\n",
    "    \n",
    "    existing_columns = set(df.columns)\n",
    "    columns_to_encode = [col for col in columns_ohe if col in existing_columns]\n",
    "\n",
    "    if columns_to_encode:\n",
    "        df = pd.get_dummies(df, columns=columns_to_encode)\n",
    "\n",
    "    return df\n",
    "\n",
    "# ohe_data(student_df)"
   ]
  },
